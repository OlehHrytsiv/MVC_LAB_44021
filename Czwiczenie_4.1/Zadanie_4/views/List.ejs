<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head', { pageTitle: 'List' }) %>
</head>
<body>
    <%- include('./partials/navigation') %>
    <h1>Students List</h1>
    <ul>
        <% students.forEach(student => { %>
            <li>
                <p><%= student.fullName %></p>
                <p><%= student.code %></p>
                <p><%= student.fieldOfStudies %></p>
            </li>
        <% }); %>
    </ul>
</body>
</html>